/* components/tab/tab.wxss */
.tab{
  height: 120rpx;
  width: 100%;
  position: fixed;
  bottom: 0;
  background-color: #ffffff;
  display: flex;
  z-index: 9999;
  align-items: center;
  font-size: 20rpx;
  justify-content: space-around;
}

.tab image{
  width: calc(274px / 10);
  height: calc(210px / 10);
  margin-bottom: 10rpx;
}

.son{
  color: "#4d4d4d";
  text-align: center;
}